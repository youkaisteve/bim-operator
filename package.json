{
    "name": "@yzw/bim-operator",
    "version": "1.2.0",
    "description": "a pack of bim operations",
    "files": [
        "build"
    ],
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "commit": "git cz",
        "lint": "tslint -p tsconfig.prod.json",
        "build": "rm -rf ./build && tsc -p tsconfig.prod.json",
        "doc": "rm -rf ./doc && npx typedoc --module commonjs --target es5 --experimentalDecorators --plugin typedoc-plugin-markdown --out ./doc ./src/",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "release": "npm run build && standard-version && git push --follow-tags && npm publish --registry http://172.16.0.132:18081/repository/npm-lab/"
    },
    "standard-version": {
        "skip": {
            "tag": true
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -e $GIT_PARAMS",
            "pre-commit": "npm run lint"
        }
    },
    "dependencies": {
        "yk-aspect": "^1.0.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@types/node": "^10.5.5",
        "husky": "^4.2.5",
        "standard-version": "^8.0.0",
        "ts-node": "^8.3.0",
        "tslib": "^1.8.1",
        "tslint": "^5.11.0",
        "tslint-midway-contrib": "1",
        "typedoc": "^0.15.6",
        "typedoc-plugin-markdown": "^2.2.16",
        "typescript": "^3.5.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "bim"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:youkaisteve/bim-operator.git"
    },
    "author": "steve.k.you",
    "license": "MIT"
}
